
#include <iostream>
using namespace std;
int main()
{
	cout << "enter calculation" << endl;
	double a, b;
	char op ;
	cin >> a >> op >> b;
	if (op == '+')
	cout << a + b << endl;
	else if (op == '-')
		cout << a - b << endl;
	else if (op == '*')
		cout << a * b << endl;
	else if (op == '/')
		cout << a / b << endl;
	return 0;
